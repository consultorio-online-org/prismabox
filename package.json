    "lint": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/generator-helper": "6.3.1",
    "@prisma/internals": "^6.3.1",
    "@sinclair/typebox": "^0.34.3",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@prisma/client": "5.22.0",
    "@types/bun": "latest",
    "esbuild": "^0.24.0",
    "prisma": "5.22.0",
    "typescript": "^5.6.3"
  }
}
