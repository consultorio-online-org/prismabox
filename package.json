{
  "name": "prismabox",
  "version": "2.2.0",
  "license": "MIT",
  "description": "Typebox generator for prisma schema",
  "author": {
    "name": "Ade Yahya Prasetyo, m1212e"
  },
  "keywords": [
    "prisma2",
    "prisma",
    "prisma-generator",
    "prisma-schema",
    "code-generation",
    "typebox",
    "typebox-generator",
    "prismabox"
  ],
  "homepage": "https://github.com/m1212e/prismabox",
  "repository": {
    "url": "https://github.com/m1212e/prismabox.git"
  },
  "scripts": {
    "dev": "bunx rimraf prisma/generated && bunx rimraf dist && bun run build && bunx prisma generate",
